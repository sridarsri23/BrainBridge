{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@assets/*": ["../attached_assets/*"],
        "@shared/*": ["../shared/*"]
      },
      "types": ["vite/client"]
    },
    "include": ["src", "../vite.config.ts"]
  }